/* Altera atributos da fonte de caracteres
   e exibe um texto na saída gráfica */
programa TextoGráfico;
inclui "gra*";
início
  /* altera atributos */
  altNomeFonte(Tela, "Arial");
  altCorFonte(Tela, COR_AZUL);
  altTamFonte(Tela, 20);
  
  /* exibe o texto */
  dsnTxt(Tela, 20, 20, "Texto na saída gráfica!");
fim.
