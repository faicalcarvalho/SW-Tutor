/* Exibe três figuras coloridas */
programa TrêsFiguras;
inclui "gra*";
início
  /* primeira figura */
  altCorPincel(Tela, COR_BRANCO);
  dsnRet(Tela, 10, 10, 150, 100);

  /* segunda */
  altCorPincel(Tela, COR_VERMELHO);
