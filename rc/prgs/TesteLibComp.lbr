programa TesteLibComp;
inclui "gra*";
inclui "evnt*";

var
  jan: Janela;
  i, j: inteiro;

  m: arranjo [0..7, 0..7] de Botão;
  b: Botão;

início
  jan := novaJanela(100, 100, 300, 300, "Teste");
  para (i := 0; i < 8; inc(i))
    para (j := 0; j < 8; inc(j))
      m[i, j] := novoBotão(jan, j*30, i*30, 30, 30, "T");
  b := novoBotão(Tela, 30, 30, 30, 30, "T");

  para (i := 0; i < 8; inc(i))
    para (j := 0; j < 8; inc(j))
      libComp(m[i, j]);
  libComp(b);
  libComp(jan);
fim.
